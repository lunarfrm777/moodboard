<div class="gallery">
    {images.map((src) => <img src={src} loading="lazy" />)}
</div>

<style>
    .gallery {
        display: grid;
        grid-template-columns: repeat(auto-fill, minmax(220px, 1fr));
        gap: 12px;
    }
    .gallery img {
        width: 100%;
        height: auto;
        object-fit: cover;
    }
</style>
